<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Disabled Group">
    <test name="Testing with a group excluded" >
        <groups>
            <run>
                <include name="gotoLinkCheck"/>
                <exclude name="displayCheck"/>
            </run>
        </groups>

        <classes>
            <class name="HomeTest"/>
            <class name="SignInTest"/>
            <class name="MembershipTest"/>
            <class name="CartTest"/>
            <class name="iFrameDemo"/>
            <class name="PharmacyTest"/>
        </classes>
    </test>
</suite>